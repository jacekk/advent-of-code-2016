{
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "rules": {
    "guard-for-in": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "default-case": 0,
    "space-unary-ops": [
      "error",
      {
        "overrides": {
          "!": true
        }
      }
    ],
    "no-console": [
      "error"
    ],
    "semi": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "no-tabs": 0
  }
}
