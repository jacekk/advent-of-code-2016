{
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "rules": {
    "space-unary-ops": [
      "error",
      {
        "overrides": {
          "!": true
        }
      }
    ],
    "no-console": [
      "error"
    ],
    "semi": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "no-tabs": 0
  }
}
